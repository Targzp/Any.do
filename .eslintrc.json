{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:vue/essential"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
      "semi": ["warn","never"],
      "indent": ["warn", 2],
      "vue/no-v-model-argument": "off",
      "vue/no-use-v-if-with-v-for": "off",
      "no-undef": 0
    }
}
